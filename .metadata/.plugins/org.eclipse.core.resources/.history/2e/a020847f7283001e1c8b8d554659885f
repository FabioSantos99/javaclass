package aula2;

public class Grade {
	
	public double grade1;
	public double grade2;
	public double grade3;
	
	public double mediaNotas() {
		
		 double media = grade1 + grade2 + grade3;
		
		if (media <= 60) {
			
			System.out.println("You've reproved");
			
		} else {
			
			System.out.println("Congratulations, You have approved");
		}
		return media;
		
	}

}
